package learnjavaismat;

public class Variablepractice {
static String name="Aunonto";

int age=15;

public static void main(String[] args) {
Variablepractice vrl=new Variablepractice()	;
vrl.age=15;
Variablepractice.name="Aunonto";

System.out.println("Variablepractice.name;");
System.out.println("vrl.age");

}
}
