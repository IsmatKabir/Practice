package learnjavaismat;

public class Nesteted {
  
//nested class is parent class
	//we are creating another child class computer inside the parent class
	
	 int age= 34;
	 static String firstname= "Aumi";
	
	public static class Computer {
		String computername="Mac";
		static int ram=16;
		
	public static void main (String args[]) {
		System.out.println("this is child class");
		//create object
		Computer myComputer=new Computer();
		System.out.println(myComputer.computername);
		System.out.println(Computer.ram);
		myComputer.computerDisplay();
	//System.out.println("this is parent class");
	
	
	}
	public void computerDisplay() {
	System.out.println("this is computer display");
		
	}	
	}
	}	
	
	
	
	
	
	
	
	
	
	
		
	

