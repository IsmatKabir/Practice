package learnjavaismat;

public class AccessModifier {

	public String name="Aumi";
	
	  String address="queens,ny";
	
	private String accountnumber="123456";
	public static void main(String args[]){
		
	AccessModifier am =new AccessModifier();
	
	System.out.println(am.name);
	System.out.println(am.address);
	System.out.println(am.accountnumber);
		
	// private u can call onlyin same class
		
	// public u can call in different class or different package
	
	
	

		
	}
	
	
	public static void display() {
		
	}
	
	
	
	
	
	
	
	
	
	
}
