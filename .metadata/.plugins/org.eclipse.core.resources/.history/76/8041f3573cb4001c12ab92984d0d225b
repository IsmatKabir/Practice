package learnjavaismat;

public class Problemvariable {
static String name="Aunonto";

int age=15;

public static void main(String[] args) {
Problemvariable vrl=new Problemvariable()	;
vrl.age=15;
Problemvariable.name="Aunonto";

System.out.println("Problemvariable.name;");
System.out.println("vrl.age");

}
}
